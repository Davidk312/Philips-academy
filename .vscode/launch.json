{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "STM32 Debug",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "external",
      "gdbTarget": "172.19.96.1:3333",
      "executable": "${workspaceFolder}/build/stm32f429zi/src/robot_hand/robot_hand.elf",
      "device": "STM32F429ZI",
      "svdFile": "${workspaceFolder}/STM32F429.svd",
      "preLaunchCommands": [
        "monitor reset halt",
        "delete breakpoints",
        "load",
        "monitor reset init"
      ],
      "breakpointType": "software"
    }
  ]
}